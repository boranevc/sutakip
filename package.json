{
  "name": "su-takip-uygulamasi",
  "version": "1.0.0",
  "description": "Su takip uygulaması - kullanıcıların su tüketimini takip edebileceği sosyal platform",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "server-sqlite": "nodemon server/index-sqlite.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index-production.js",
    "start:prod": "NODE_ENV=production node server/index-production.js",
    "build:prod": "cd client && npm run build && npm run build:server",
    "build:server": "echo 'Server build completed'",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "su",
    "takip",
    "sağlık",
    "sosyal"
  ],
  "author": "Su Takip Uygulaması",
  "license": "MIT"
}
